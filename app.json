{
  "expo": {
    "name": "No Food Wasted",
    "slug": "NoFoodWasted",
    "description": "The NoFoodWasted application",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "githubUrl": "https://github.com/de-sigaar/NoFoodWasted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon/icon.png",
    "splash": {
      "image": "./assets/images/icon/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "CAMERA"
      ]
    }
  }
}
